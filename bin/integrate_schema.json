{
  		"schema": {
                "integrate": {
                    "type": "object",
                    "title": "integrate_datasets",                    
                        "properties": {
                            "method": {
                                "type": "string",
                                "title": "method",
                                "default": "Liger"
                            },
                            "unique_id_var": {
                                "type": "string",
                                "title": "unique_id_var",
                                "default": "manifest"
                            },
                            "take_gene_union": {
                                "type": "boolean",
                                "title": "take_gene_union",
                                "default": false
                            },
                            "remove_missing": {
                                "type": "boolean",
                                "title": "remove_missing",
                                "default": true
                            },
                            "num_genes": {
                                "type": "integer",
                                "title": "num_genes",
                                "default": 3000
                            },
                            "combine": {
                                "type": "string",
                                "title": "combine",
								"enum": ["union", "intersect"],
                                "default": "union"
                            },
                            "keep_unique": {
                                "type": "boolean",
                                "title": "keep_unique",
                                "default": false
                            },
                            "capitalize": {
                                "type": "boolean",
                                "title": "capitalize",
                                "default": false
                            },
                            "do_plot": {
                                "type": "boolean",
                                "title": "do_plot",
                                "default": false
                            },
                            "cex_use": {
                                "type": "number",
                                "title": "cex_use",
                                "default": 0.3
                            },
                            "use_cols": {
                                "type": "boolean",
                                "title": "use_cols",
                                "default": true
                            },
							"k": {
                                "type": "integer",
                                "title": "k",
                                "default": 30
                            },
					        "lambda": {
                                "type": "number",
                                "title": "lambda",
                                "default": 5.0
                            },
							"thresh": {
                                "type": "number",
                                "title": "thresh",
                                "default": 1e-4
                            },
							"max_iters": {
                                "type": "integer",
                                "title": "max_iters",
                                "default": 100
                            },
							"nrep": {
                                "type": "integer",
                                "title": "nrep",
                                "default": 1
                            },
							"H_init": {
                                "type": "number",
                                "title": "H_init",
                                "default": "null"
                            },
							"W_init": {
                                "type": "number",
                                "title": "W_init",
                                "default": "null"
                            },
							"V_init": {
                                "type": "number",
                                "title": "V_init",
                                "default": "null"
                            },
							"rand_seed": {
                                "type": "integer",
                                "title": "rand_seed",
                                "default": 1
                            },
							"print_obj": {
                                "type": "boolean",
                                "title": "print_obj",
                                "default": false
                            },
							"knn_k": {
                                "type": "integer",
                                "title": "knn_k",
                                "default": 20
                            },
							"k2": {
                                "type": "integer",
                                "title": "k2",
                                "default": 500
                            },
							"prune_thresh": {
                                "type": "number",
                                "title": "prune_thresh",
                                "default": 0.2
                            },
							"ref_dataset": {
                                "type": "string",
                                "title": "ref_dataset",
                                "default": "NULL"
                            },
							"min_cells": {
                                "type": "integer",
                                "title": "min_cells",
                                "default": 2
                            },
							"quantiles": {
                                "type": "integer",
                                "title": "quantiles",
                                "default": 50
                            },
							"nstart": {
                                "type": "integer",
                                "title": "nstart",
                                "default": 10
                            },
							"resolution": {
                                "type": "number",
                                "title": "resolution",
                                "default": 1
                            },
							"dims_use": {
                                "type": "number",
                                "title": "dims_use",
                                "default": ""
                            },
							"dist_use": {
                                "type": "string",
                                "title": "dist_use",
                                "default": "CR"
                            },
							"center": {
                                "type": "string",
                                "title": "center",
                                "default": false
                            },
							"small_clust_thresh": {
                                "type": "number",
                                "title": "small_clust_thresh",
                                "default": 0
                            },
							"id_number": {
                                "type": "number",
                                "title": "id_number",
                                "default": ""
                            },
							"print_mod": {
                                "type": "boolean",
                                "title": "print_mod",
                                "default": false
                            },
							"print_align_summary": {
                                "type": "boolean",
                                "title": "print_align_summary",
                                "default": false
                            }
                        }
                }
            }
}